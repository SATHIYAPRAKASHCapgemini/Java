package work;
public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// declares an Array of integers.
				Student[] arr;
				arr = new Student[5];
				arr[0] = new Student(1,"Aakash");
				arr[1] = new Student(2,"Dinesh");
				arr[2] = new Student(3,"Karthi");
				arr[3] = new Student(4,"Suresh");
				arr[4] = new Student(5,"Yogesh");
				for (int i = 0; i < arr.length; i++)
					System.out.println("Element at " + i + " : " +arr[i].roll_no +" "+ arr[i].name);
	}

}
class Student
{
	public int roll_no;
	public String name;
	Student(int roll_no, String name)
	{
		this.roll_no = roll_no;
		this.name = name;
	}
}
